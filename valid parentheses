bool isvalid(char* s)
{
    int len = strlen(s);
    char* stack = (char*) malloc(len * sizeof(char));
    int top = -1;  
    for (int i = 0; i < len; i++) {
        char l = s[i];
        if (l == '(' || l == '{' || l == '[')
         {
            stack[++top] = l;
        }
        else
        {
            if (top == -1)
            {
                free(stack);
                return false; 
            }
            char o = stack[top--];
            if ((c == ')' && o!= '(') ||(c == '}' && o!= '{') ||(c == ']' && o!= '['))
            {
                free(stack);
                return false; 
            }
        }
    }
    bool isvalid = (top == -1);
    free(stack);
    return isvalid;
}
